{
  "name": "sponsored-remover",
  "description": "remove sponsored items on Amazon search results",
  "version": "0.0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://www.amazon.co.jp/b*",
        "https://www.amazon.co.jp/s*",
        "https://www.amazon.co.jp/kindle-dbs/manga-store/free"
      ],
      "js": ["script.js"]
    }
  ]
}
